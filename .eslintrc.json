{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": ["off", { "allow": ["foo_", "_bar"] }],
    "linebreak-style": "off",
    "consistent-return": "off",
    "no-prototype-builtins": "off",
    "import/no-extraneous-dependencies": "off"
  },
  "ignorePatterns": ["*.test.js"]
}
